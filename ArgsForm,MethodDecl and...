                        {
                            getInput();
                            if(input.equals("number"))
                            {
                                getInput();
                                if(input.equals(")"))
                                {
                                    getInput();
                                    if(input.equals(";"))
                                    {
                                        getInput();
                                        return true;
                                    }
                                    else
                                    {
