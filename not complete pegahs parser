    }
    private static boolean Term()
    {//problem
        boolean result=false;
        //if(first(Factor56456565)
        if(input.equals("identifier")||input.equals("number")||input.equals("Terminal")||input.equals("new")||input.equals("("))  {
            if(Factor())
            {
                result=true;

                if(input.equals("/")||input.equals("%")||input.equals("*"))
                {
                    result=false;
                    while(Mulop())
                    {
                        if(input.equals("identifier")||input.equals("number")||input.equals("Terminal")||input.equals("new")||input.equals("(")) { //first(Factor)
                            if(Factor())
                            {
                                result=true;
                                continue;
                            }
                            else
                            {
                                result=true;
                                error("Factor expected here after mulop in col: "+token.getCol() + "in line: " + token.getLine());
                                while(!isFollow("Term",input))
                                {
                                    getInput();
                                }
                                break;
                                //error occures:|
                            }
                        }
                        else
                        {
                            error("enter factor after mulop please in col "+ token.getLine() + "in line " + token.getLine());
                            result=true;
                            while(!isFollow("Term",input))
                            {
                                getInput();
                            }
                            break;
                        }
                    }
                }
                else
                {
                    //do nothing. no loop occures here.
                }
            }
            else
            {
                error("wrong Factor in col: "+token.getCol() + "in line: " + token.getLine());
                while(!isFollow("Term",input))
                {
                    getInput();
                }
                result=true;
            }
        }
        else
        {
            result=true;
            error("first Factor expected here in line" +token.getLine() + "in col" + token.getCol() );
            while(!isFollow("Term",input))
            {
                getInput();
            }
        }
        //System.out.println("after term s finished result is "+result);
        return result;
    }  //not checked ep
    private static boolean Type()
    {
        if(input.equals("char") || input.equals("float") || input.equals("int"))
        {
            getInput();
            if(input.equals("["))
            {
                getInput();
                if(input.equals("]"))
                {
                    getInput();
                    return true;
                }
                else
                {
                    error("missing ] in line: " + token.getLine() +"in Col : "+token.getCol());
                    while(!isFollow("Type",input))
                    {
                        getInput();
                    }
                    return true;
                }
            }
            else
            {
                return true;
            }
        }
        return false;
    } //checked //not checked ep
    private static boolean VarDecl()
    {
        if(input.equals("int") || input.equals("float") || input.equals("char"))//first(Type)
        {
            if(Type())
            {
                if(input.equals("identifier"))
                {
                    getInput();
                    while((!input.equals(";")))
                    {
                        if(input .equals(","))
                        {
                            getInput();
                            if(input.equals("identifier"))
                            {
                                getInput();
                                continue;
                            }
                            else
                            {
                                error("needed identifier in line "+token.getLine()+"and col "+token.getCol());
                                while(!isFollow("VarDecl",input))
                                {
                                    getInput();
                                }
                                return true;   //id ,,
                            }
                        }
                        else
                        {
                            while(!isFollow("VarDecl",input))
                            {
                                getInput();
                            }
                            error("missing , or ; in line "+token.getLine() + " in col " +token.getCol());
                            return true;  //id!
                        }
                    }
                    getInput();
                    return true;
                }
                else
                {
                    error("needed identifier in line "+token.getLine()+"and col "+token.getCol());
                    while(!isFollow("VarDecl",input))
                    {
                        getInput();
                    }
                    return true;
                }
            }

            return false;
        }
        else
        {
            error("no Type in VarDecl in line "+token.getLine() + "in col" + token.getCol());
            return true;
        }
    } //checked //error production completed





}//end class
